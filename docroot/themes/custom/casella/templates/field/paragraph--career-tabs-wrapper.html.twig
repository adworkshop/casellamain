{{ attach_library('casella/jquery.zozotabs') }}
{% set tab_anchor = content.field_career_tabs_content %}
<!--Interactive Content-->
<div class="internal-interActiveContent-container">

  <div class="relativeWrapper">
    <a class="deeplink_target" name="tabs" id="tabs"></a>
  </div>

  <!--Zozo Tabs-->
  <div id="tabbed-nav">

    <!-- Tab Navigation Menu -->

    <ul>
      {#<li id="Local"><a>Local</a></li>
      <li id="Regional"><a>Regional</a></li>#}


      {% for tab_anchor in tab_anchors_2 %}
        <li id="{{ tab_anchor|adw_machine_name }}"><a>{{ tab_anchor }}</a></li>
      {% endfor %}
    </ul>

    <!--Slide Panel Container-->
    <div>
      {{ content.field_career_tabs_content }}
    </div>

  </div>
</div>
