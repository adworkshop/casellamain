  RewriteRule .* https://www.casella.com/casella-organics [L,R=301]

  RewriteCond %{HTTP_HOST} ^m\.casellaorganics\.com [NC]
  RewriteRule .* https://www.casella.com/casella-organics [L,R=301]

  RewriteCond %{HTTP_HOST} earthlifegrows.com [NC]
